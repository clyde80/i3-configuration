! Rofi configuration
rofi.modi: run

rofi.color-enabled: true

!                  Background   Foreground  Background Alternate    Highlight Background    Highlight Foreground
rofi.color-window: #0B0E1C,     #F17468,    #0B0E1C     
rofi.color-normal: #0B0E1C,     #ffe7cb,    #0B0E1C,                #F17468,                #0B0E1C
rofi.color-active: #0B0E1C,     #ffe7cb,    #0B0E1C,                #F17468,                #0B0E1C
rofi.color-urgent: #0B0E1C,     #ffee95,    #0B0E1C,                #ffee95,                #0B0E1C

rofi.columns: 1
rofi.fake-transparency: true
rofi.hide-scrollbar: true
rofi.disable-history: true
rofi.bw: 0
rofi.fullscreen: false
rofi.lines: 5
rofi.padding: 5
rofi.separator-style: none
